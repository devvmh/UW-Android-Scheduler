<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.callysto.devin.skedulr"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".SkedulrActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.callysto.devin.skedulr.CreateAccountActivity"
            android:label="@string/create_account" />
        <activity
            android:name="com.callysto.devin.skedulr.LoginActivity"
            android:label="@string/login" />
        <activity
            android:name="com.callysto.devin.skedulr.CourseCatalogActivity"
            android:label="@string/course_catalog" />
        <activity
            android:name="com.callysto.devin.skedulr.AddSectionActivity"
            android:label="@string/add_section" />
    </application>

</manifest>